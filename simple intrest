p=int(input("Enter the principle amount:"))
n=int(input("Enter the number of yerar:"))
SC=input("Senior citizen yes/No:")
G=input("Male/Female:")
if SC=='Y' and G=='M':
    print("SI=",(p*n*12)/100)
elif SC=='Y' and G=='F':
   print("SI=",(p*n*15)/100)
else:
    print("SI=",(p*n*10)/100)


